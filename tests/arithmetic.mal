; +
(assert (= (+ 1 -4) -3))
(assert (= (+ 1 1) 2))

; -
(assert (= (- 2 1) 1))
(assert (= (- 1 2) -1))

; *
(assert (= (* 2 3) 6))
(assert (= (* -2 3) -6))
(assert (= (* -2 -3) 6))

; /
(assert (= (/ 3 2) 1))
(assert (= (/ 2 3) 0))

; mod
(assert (= (mod 10 4) 2))
(assert (= (mod 4 10) 4))

; >
(assert (> 3 2))
(assert (not (> 1 2)))
(assert (not (> 1 1)))

; <
(assert (< 1 3))
(assert (not (< 3 2)))
(assert (not (< 1 1)))

; >=
(assert (>= 3 1))
(assert (not (>= 1 2)))
(assert (>= 1 1))

; <=
(assert (<= 1 3))
(assert (not (<= 3 2)))
(assert (<= 1 1))