; +
(assert (= (+) 0))
(assert (= (+ 1) 1))
(assert (= (+ 1 1) 2))
(assert (= (+ 1 2 3 4) 10))

; -
(assert (= (-) 0))
(assert (= (- 1) -1))
(assert (= (- 1 2) -1))
(assert (= (- 10 1 2 3) 4))

; *
(assert (= (*) 1))
(assert (= (* 2) 2))
(assert (= (* 2 3) 6))
(assert (= (* -2 3) -6))
(assert (= (* -2 -3) 6))
(assert (= (* 10 1 2 3) 60))

; /
(assert (= (/) 1))
(assert (= (/ 1) 1))
(assert (= (/ 2) 0))
(assert (= (/ 3 2) 1))
(assert (= (/ 128 2 4) 16))

; >
(assert (not (>)))
(assert (> 1))
(assert (> 3 2 1))
(assert (not (> 3 1 2)))
(assert (not (> 1 1)))
; <
(assert (not (<)))
(assert (< 1))
(assert (< 1 2 3))
(assert (not (< 1 3 2)))
(assert (not (< 1 1)))
; >=
(assert (not (>=)))
(assert (>= 1))
(assert (>= 3 2 1))
(assert (not (>= 3 1 2)))
(assert (>= 1 1))
; <=
(assert (not (<=)))
(assert (<= 1))
(assert (<= 1 2 3))
(assert (not (<= 1 3 2)))
(assert (<= 1 1))